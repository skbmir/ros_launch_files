<launch>
    <node pkg="tf" type="static_transform_publisher" 
          name="zed_tf_broadcaster"
          args="$(arg tf_params) base_link zed_link"/>
    <include file="$(env HOME)/workspace/ros_launch_files/ublox.launch">
        <arg name="node_name"       value="ublox_node"/>
        <arg name="param_file_name" value="$(env HOME)/workspace/ros_launch_files/ublox_car.yaml"/>
        <arg name="tf_params"       value="0 0 0 0 0 0"/>
    </include>
    <include file="$(env HOME)/workspace/ros_launch_files/urg.launch">
        <arg name="serial_port" value="/dev/ttyACM0"/>
        <arg name="angle_min"   value="-1.5708"/>
        <arg name="angle_max"   value="1.5708"/>
        <arg name="tf_params"   value="0 0 0 0 0 0"/>
    </include>
    <include file="$(env HOME)/workspace/ros_launch_files/D415.launch">
        <arg name="camera"    value="D415_1"/>
        <arg name="serial_no" value="840412061584"/>
        <arg name="tf_params" value="0 0 0 0 0 0"/>
    </include>
    <include file="$(env HOME)/workspace/ros_launch_files/D415.launch">
        <arg name="camera"    value="D415_2"/>
        <arg name="serial_no" value=""/>
        <arg name="tf_params" value="0 0 0 0 0 0"/>
    </include>
    <include file="$(env HOME)/workspace/ros_launch_files/D415.launch">
        <arg name="camera"    value="D415_3"/>
        <arg name="serial_no" value=""/>
        <arg name="tf_params" value="0 0 0 0 0 0"/>
    </include>
    <node pkg="rqt_reconfigure" type="rqt_reconfigure" name="rqt_reconfigure"/>
</launch>